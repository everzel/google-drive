{
    "name": "everzel/google-drive",
    "description": "Package for Google Drive",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Everzel\\GoogleDrive\\": "src/"
        }
    },
    "authors": [
        {
            "name": "zelvad",
            "email": "vladislav.zelenyk@scalemeup.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/filesystem": "*",
        "nao-pon/flysystem-google-drive": "^1.1",
        "illuminate/collections": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Everzel\\GoogleDrive\\GoogleDriveServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "phpstan/phpstan": "^1.4"
    }
}
